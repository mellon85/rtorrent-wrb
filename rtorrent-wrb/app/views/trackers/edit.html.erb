<h1>Editing tracker</h1>

<% form_for(@tracker) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :torrent_hash %><br />
    <%= f.text_field :torrent_hash %>
  </p>
  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @tracker %> |
<%= link_to 'Back', trackers_path %>
